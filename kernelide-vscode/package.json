{
  "name": "kernelide",
  "displayName": "KernelIDE",
  "description": "GPU Kernel Development with Modal.com - Execute CUDA, Triton, CUTLASS, and Mojo kernels on cloud GPUs",
  "version": "0.1.0",
  "publisher": "kernelide",
  "repository": {
    "type": "git",
    "url": "https://github.com/kernelide/kernelide-vscode"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "kernelide-sidebar",
          "title": "KernelIDE",
          "icon": "media/kernelide.svg"
        }
      ]
    },
    "views": {
      "kernelide-sidebar": [
        {
          "type": "webview",
          "id": "kernelide.sidebarView",
          "name": "KernelIDE",
          "contextualTitle": "KernelIDE"
        }
      ]
    },
    "commands": [
      {
        "command": "kernelide.runKernel",
        "title": "KernelIDE: Run Kernel on GPU"
      },
      {
        "command": "kernelide.configure",
        "title": "KernelIDE: Configure Modal Settings"
      }
    ],
    "configuration": {
      "title": "KernelIDE",
      "properties": {
        "kernelide.modalEndpoint": {
          "type": "string",
          "default": "",
          "description": "Modal.com endpoint URL for kernel execution"
        },
        "kernelide.defaultGpu": {
          "type": "string",
          "default": "T4",
          "enum": ["T4", "L4", "A10", "A100-40GB", "A100-80GB", "L40S", "H100", "H200", "B200"],
          "description": "Default GPU type for kernel execution"
        },
        "kernelide.defaultLanguage": {
          "type": "string",
          "default": "cuda",
          "enum": ["cuda", "triton", "cutlass", "cutedsl", "mojo"],
          "description": "Default kernel language/DSL"
        },
        "kernelide.timeout": {
          "type": "number",
          "default": 30,
          "description": "Execution timeout in seconds (5-300)"
        }
      }
    },
    "keybindings": [
      {
        "command": "kernelide.runKernel",
        "key": "ctrl+shift+k",
        "mac": "cmd+shift+k"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
